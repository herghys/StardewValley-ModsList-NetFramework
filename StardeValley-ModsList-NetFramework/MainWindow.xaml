<Window x:Class="StardewValleyModList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StardewValleyModList"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <TabControl FontSize="15" Margin="10,0,10,10">
            <TabItem x:Name="header_MyMods" Header="My Mods" Width="150" IsSelected="True" />
            <TabItem x:Name="header_OtherPlayerMods" Header="Other Player" Width="150"/>
            <TabItem x:Name="header_ModsComparison" Header="Mod Compare" Width="150"/>

        </TabControl>

        <Frame x:Name="MainContent" Margin="0,30,20,10">

        </Frame>
    </Grid>
</Window>

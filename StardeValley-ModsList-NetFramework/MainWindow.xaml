<Window x:Class="StardewValleyModList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StardewValleyModList"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <TabControl FontSize="15" Margin="10,0,10,10" SelectionChanged="TabControl_SelectionChanged">
            <TabItem x:Name="header_MyMods" Header="My Mods" Width="150" TabIndex="0">
                <Frame x:Name="myModsFrame"/>
            </TabItem>
            <TabItem x:Name="header_OtherPlayerMods" Header="Other Player" Width="150" TabIndex="1">
                <Frame x:Name="otherPlayerModsFrame"/>
            </TabItem>
            <TabItem x:Name="header_ModsComparison" Header="Non Exist Mods" Width="150" TabIndex="2">
                <Frame x:Name="modsComparisonFrame"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
